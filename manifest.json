{
  "manifest_version": 3,
  "name": "PsuedoCache",
  "version": "1.0.1",
  "author": "Kennan Budnik, Stephen Lee",
  "description": "Hopefully this works",
  "permissions": [
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.js" ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html",
    "default_title": "A tooltip popup description when you hover over the Chrome Extension icon."
  }
}
